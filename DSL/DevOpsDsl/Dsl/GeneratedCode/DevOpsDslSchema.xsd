  <!-- ApplicationHasDevopsCategories Moniker -->
  <xsd:element name="applicationHasDevopsCategoriesMoniker" type="ApplicationHasDevopsCategoriesMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="ApplicationHasDevopsCategoriesMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for ApplicationHasDevopsCategories instance.</xsd:documentation>
    </xsd:annotation>
