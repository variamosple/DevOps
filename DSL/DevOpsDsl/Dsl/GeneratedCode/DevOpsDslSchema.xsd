  <xsd:complexType name="DevopsCategory">
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
        <xsd:sequence minOccurs="0" maxOccurs="1">
          <!-- Relationship: DevopsCategoryHasDevopsPracticed -->
          <xsd:element name="devopsPracticed" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>Instances of DevopsCategoryHasDevopsPracticed</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element ref="devopsCategoryHasDevopsPracticed" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                    <xsd:documentation>[Instance of DevopsCategoryHasDevopsPracticed needs to be serialized in full-form]</xsd:documentation>
                  </xsd:annotation>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <!-- Property: CategoryName -->
        <xsd:attribute name="categoryName" type="xsd:string" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- DevopsCategory Moniker -->
  <xsd:element name="devopsCategoryMoniker" type="DevopsCategoryMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="DevopsCategoryMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for DevopsCategory instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation>Attribute to store the moniker string.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- DevopsPractice -->
  <xsd:element name="devopsPractice" type="DevopsPractice" substitutionGroup="core:modelElement" />
  <xsd:complexType name="DevopsPractice">
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <!-- Property: PracticeName -->
        <xsd:attribute name="practiceName" type="xsd:string" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- DevopsPractice Moniker -->
  <xsd:element name="devopsPracticeMoniker" type="DevopsPracticeMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="DevopsPracticeMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for DevopsPractice instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation>Attribute to store the moniker string.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- DevOpsHasApplication -->
  <xsd:element name="devOpsHasApplication" type="DevOpsHasApplication" substitutionGroup="core:modelElement" />
  <xsd:complexType name="DevOpsHasApplication">
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
        <xsd:sequence minOccurs="0" maxOccurs="1">
          <!-- Target end Application -->
          <xsd:element ref="application" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>[Target role-player]</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- DevOpsHasApplication Moniker -->
  <xsd:element name="devOpsHasApplicationMoniker" type="DevOpsHasApplicationMoniker" substitutionGroup="core:modelElementMoniker" />
  <xsd:complexType name="DevOpsHasApplicationMoniker">
    <xsd:annotation>
      <xsd:documentation>Moniker for DevOpsHasApplication instance.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="core:ModelElementMoniker">
        <xsd:attribute name="Id" type="xsd:string" >
          <xsd:annotation>
            <xsd:documentation>Attribute to store the moniker string.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  
  <!-- ApplicationHasDevopsCategories -->
  <xsd:element name="applicationHasDevopsCategories" type="ApplicationHasDevopsCategories" substitutionGroup="core:modelElement" />
  <xsd:complexType name="ApplicationHasDevopsCategories">
    <xsd:complexContent>
      <xsd:extension base="core:ModelElement">
        <xsd:sequence minOccurs="0" maxOccurs="1">
          <!-- Target end DevopsCategory -->
          <xsd:element ref="devopsCategory" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
              <xsd:documentation>[Target role-player]</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <!-- Id -->
        <xsd:attribute name="Id" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>Instance Guid of this element, needed because SerializeId is set to true.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
